<Window x:Class="GUI.SelectTableWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="SelectTableWindows" Height="503" Width="800"
        WindowStartupLocation="CenterScreen"
         ResizeMode="NoResize">
    <Grid Style="{StaticResource GridBackground}">
       
        <WrapPanel Margin="50,36,0,0" Width="454" HorizontalAlignment="Center" VerticalAlignment="Top">
            <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="Wybierz tabelę" VerticalAlignment="Center" FontSize="40" TextAlignment="Center" Foreground="#FFFFF6F6" FontWeight="Bold" Width="396"/>
        </WrapPanel>
        <WrapPanel Margin="60,112,0,-19" HorizontalAlignment="Center" Width="376" >

            <Button Content="Egzempalrze" Style="{StaticResource SelectTableButton}" HorizontalAlignment="Center" Click="Button_Click_1"/>
            <Button Content="Klienci"  Style="{StaticResource SelectTableButton}" Click="Button_Click"/>
            <Button x:Name="modelsTableButton" Content="Modele"  Style="{StaticResource SelectTableButton}" Click="modelsTableButton_Click"/>
            <Button Content="Pracownicy"  Style="{StaticResource SelectTableButton}" Click="Button_Click_2"/>
            <Button Content="Rezerwacje"  Style="{StaticResource SelectTableButton}" Click="Button_Click_3"/>
            <Button Content="Wypozyczone"  Style="{StaticResource SelectTableButton}" Click="Button_Click_4"/>

        </WrapPanel>

        <Button x:Name="ReturnToLoginButton" Content="Wyloguj"  Width="112" FontSize="18" Click="ReturnToLoginButton_Click" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="10,0,0,10" />



    </Grid>
</Window>
